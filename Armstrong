#include<stdio.h>
int main (){
int num,sum=0,count=0;
printf("Enter your Number: ");
    scanf("%d",&num);
    int temp=num;
    while(temp !=0){
        temp = temp/10;
        count=count+1;
    }
    temp = num;
    while(temp !=0){
       int lastdigit = temp%10;
        sum = sum +pow(lastdigit, count);
        temp = temp/10;
    }
    if(num == sum)
        printf("Armstrong");
    else
        printf("Not Armstrong");
    return 0;

}
