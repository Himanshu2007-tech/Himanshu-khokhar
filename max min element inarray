#include<stdio.h>
int main(){
int x,i;
    scanf("%d",&x);
    int list[x];
    for(i=1; i<=x;i++){
        scanf("%d",&list[i]);
    }
    int max= list[0];
    int min= list[0];
    for(i=1;i<=x;i++){
        if(list[i] > max)
            max = list[i];
        if(list[i] < min)
            min = list[i];
        }
    printf("%d %d",min,max);
    return 0;
}
