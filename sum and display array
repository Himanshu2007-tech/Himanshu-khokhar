#include<stdio.h>
int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int x,i,sum=0;
    scanf("%d",&x);
    int list[x];
    for(i=0;i<=x;i++){
        scanf("%d",&list[i]);
    }
    for(i=0;i<x;i++){
        sum=sum+list[i];
        
    }
    printf("%d ",sum);
    return 0;
}
