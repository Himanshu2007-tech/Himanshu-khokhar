#include <stdlib.h>

int main() {
    int R,Col,sum=0;
    printf("Enter row size");
    scanf("%d",&R);
    printf("Enter column size");
    scanf("%d",&Col);
    int list[R][Col];
    
    for(int i=0;i<R;i++){
        for(int j=0;j<Col;j++){
printf("Enter Value..");
            scanf("%d",&list[i][j]);
        }
    }
    for(int t=0;t<R;t++){
        for(int p=0;p<Col;p++){
        printf("%d",list[t][p]);
        sum=sum+list[t][p];
        }
    }
    printf("The sum os value is %d",sum);
    return 0;
}
